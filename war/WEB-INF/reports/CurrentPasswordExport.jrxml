    <queryString>
        <![CDATA[select p.name as Title, p.username as Username, pd.password as Password, p.notes as Notes, t.name as Tag from passwords p left outer join password_data pd on p.id = pd.password_id left outer join password_tags pt on p.id=pt.password_id left outer join tags t on t.id=pt.tag_id where pd.password_position=0 and p.active='Y' order by p.name, t.name]]>
